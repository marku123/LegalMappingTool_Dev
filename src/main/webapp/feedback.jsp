<!-- Sidebar feedback tab. -->
<div id="sidebartab">Feedback</div>


<!-- The Feedback Modal -->
<div id="myFeedbackModal" class="feedbackmodal">

  <!-- Modal content -->
  <div class="feedback-modal-content">
    <div class="feedback-modal-header">
      <span class="modalclose">×</span>
      <p>Legal Mapping Tool Prototype Feedback Form</p>
    </div>
    <div class="feedback-modal-body">
             <p>Please feel free to provide us with comments or suggestions you have on any component of the Legal Mapping Tool prototype.<br>
             </p>
             <p>Name (Optional): <input type="text" id="feedbackname"></p>
             <p id="commentstitle">Comments:</p> <textarea id="feedbacktextarea"></textarea>                     

             
             <button id='submitFeedback'>Submit Feedback</button>
     </div>
     <div class="feedback-modal-body-response">
            <p id="feedback-response-text"></p>
     </div>
    <div class="feedback-modal-footer">
    </div>
  </div>
</div>
